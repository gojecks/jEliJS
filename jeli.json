{
    "projectName": "jeli",
    "build": {
        "common": {
            "folder": "common",
            "entry": "module",
            "dest": "./dist/common/"
        },
        "form": {
            "entry": "module",
            "folder": "form",
            "dest": "./dist/form/"
        },
        "http": {
            "entry": "module",
            "folder": "http",
            "dest": "./dist/http/"
        },
        "route": {
            "entry": "module",
            "folder": "route",
            "dest": "./dist/route/"
        },
        "datetime": {
            "entry": "module",
            "folder": "datetime",
            "dest": "./dist/datetime/"
        },
        "core": {
            "dest": "./dist/",
            "folder": "core",
            "entry": "core",
            "concat": [
                "../jeli.helpers/common/*.js",
                "../jeli.helpers/eval.js",
                "../jeli.helpers/copy.js",
                "../jeli.helpers/promise/*.js",
                "../jeli.helpers/events/emitter.js",
                "../jeli.helpers/externalScriptLoader.js",
                "../jeli.helpers/serializer.js",
                "../jeli.helpers/xhr/*.js"
            ]
        },
        "test": {
            "dest": "./docs/examples/Todo/app.js",
            "src": "./docs/examples/Todo/scripts/main.js",
            "options": {
                "parseModule": true
            }
        }
    }

}